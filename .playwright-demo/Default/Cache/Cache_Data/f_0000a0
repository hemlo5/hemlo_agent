$(document).ready(function() {

                                var owl = $('#owl-demo-16');
                                owl.owlCarousel({
                                  items: 1,
                                  loop: true,
                                  margin: 0,
                                  autoplay: true,
                                  autoplayTimeout: 8000,
                                  autoplayHoverPause: false,
                          autoWidth:false,
                                  margin: 10,
                                 animateIn: 'fadeIn',
              animateOut: 'fadeOut',
                          mouseDrag: true,
                                  responsiveClass: true,
                                  responsive: {
                                    0: {
                                      items: 1,
                                      nav: false
                                    },
                                    500: {
                                      items: 1,
                                      nav: false
                                    },
                            700: {
                                      items: 1,
                                      nav: false
                                    },
                            1000: {
                                      items: 1,
                                      nav: false,
                                      margin: 20
                                    },
                            1300: {
                                      items: 1,
                                      nav: false,
                                      margin: 20
                                    },
                            1600: {
                                      items: 1,
                                      nav: false,
                                      margin: 20
