$(document).ready(function() {

                                var owl = $('#owl-demo-18');
                                owl.owlCarousel({
                                  items: 4,
                                  loop: false,
                                  margin: 0,
                                  autoplay: false,
                                  autoplayTimeout: 5000,
                                  autoplayHoverPause: true,
                          autoWidth:false,
                                  margin: 10,
                          
                          mouseDrag: true,
                                  responsiveClass: true,
                                  responsive: {
                                    0: {
                                      items: 1,
                                      nav: false
                                    },
                                    500: {
                                      items: 1,
                                      nav: false
                                    },
                            700: {
                                      items: 2,
                                      nav: false
