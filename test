				// Sanitize before GA loads
				(function sanitizeTrackingParams() {
					const url = new URL(window.location.href);
					const keys = ['wbraid', 'gclid', 'gbraid', 'fbclid', 'msclkid', 'ttclid', 'dclid'];
					let changed = false;

					function isSuspicious(value) {
						const decoded = decodeURIComponent(value);
						// Valid values should be at least 30 characters long and contain only safe characters
						return !/^[A-Za-z0-9_-]{25,}={0,2}$/.test(decoded);
					}

					keys.forEach(key => {
						const val = url.searchParams.get(key);
						if (val && isSuspicious(val)) {
							url.searchParams.delete(key);
							changed = true;
						}
					});

					if (changed) {
						window.history.replaceState({}, '', url.toString());
					}
				})();
		</script>
